{
  "name": "tsc-express-node",
  "version": "1.0.0",
  "description": "A simple express API which will serve authentication tokens",
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run lint && tsc",
    "dev": "concurrently -rk \"npm run build -- --watch \" \"cross-env NODE_ENV=development nodemon ./lib/index.js\"",
    "start": "cross-env NODE_ENV=production node ./lib/index.js",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "node",
    "typescript",
    "auth0",
    "express"
  ],
  "author": "Daniel Verdejo",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.3.12",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.3.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.11",
    "nodemon": "^1.18.10",
    "ts-node": "^7.0.1",
    "tsc-init": "^2.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.1"
  }
}
